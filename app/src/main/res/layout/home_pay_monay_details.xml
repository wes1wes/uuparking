<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <View
        android:id="@+id/view"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="16dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/radios_shadow"
        app:layout_constraintBottom_toTopOf="@+id/btn_pay_monay_details_sure"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/btn_pay_monay_details_sure"
        android:layout_width="0dp"
        android:layout_height="40dp"
        android:layout_marginEnd="32dp"
        android:layout_marginStart="32dp"
        android:text="去结算"
        android:textColor="@color/colorWhite"
        android:background="@drawable/bg_rectangle_rounded_blue"
        app:layout_constraintBottom_toTopOf="@+id/textView56"
        app:layout_constraintEnd_toEndOf="@+id/view"
        app:layout_constraintStart_toStartOf="@+id/view" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:gravity="center"
        android:text="费用结算"
        android:textColor="@color/uuImportantText"
        app:layout_constraintEnd_toStartOf="@+id/imageView22"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/imageView21"
        app:layout_constraintTop_toTopOf="@+id/view" />

    <TextView
        android:id="@+id/tv_pay_monay_details_order_id"
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="8dp"
        android:gravity="center_vertical"
        android:text="订单编号："
        android:textColor="@color/uuImportantText"
        app:layout_constraintEnd_toStartOf="@+id/tv_pay_monay_details_order_id_show"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="@+id/view"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <TextView
        android:id="@+id/tv_pay_monay_details_lock_id"
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="1dp"
        android:gravity="center_vertical"
        android:text="地锁编号："
        android:textColor="@color/uuImportantText"
        app:layout_constraintEnd_toStartOf="@+id/tv_pay_monay_details_lock_id_show"
        app:layout_constraintStart_toStartOf="@+id/view"
        app:layout_constraintTop_toBottomOf="@+id/tv_pay_monay_details_order_id" />

    <TextView
        android:id="@+id/tv_pay_monay_details_location"
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="1dp"
        android:gravity="center_vertical"
        android:text="车位地址："
        android:textColor="@color/uuImportantText"
        app:layout_constraintEnd_toStartOf="@+id/tv_pay_monay_details_location_show"
        app:layout_constraintStart_toStartOf="@+id/view"
        app:layout_constraintTop_toBottomOf="@+id/tv_pay_monay_details_lock_id" />

    <TextView
        android:id="@+id/tv_pay_monay_details_time"
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="1dp"
        android:gravity="center_vertical"
        android:text="共享时间："
        android:textColor="@color/uuImportantText"
        app:layout_constraintEnd_toStartOf="@+id/tv_pay_monay_details_time_show"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="@+id/view"
        app:layout_constraintTop_toBottomOf="@+id/tv_pay_monay_details_location" />

    <TextView
        android:id="@+id/tv_pay_monay_details_price"
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="1dp"
        android:gravity="center_vertical"
        android:text="共享价格："
        android:textColor="@color/uuImportantText"
        app:layout_constraintEnd_toStartOf="@+id/tv_pay_monay_details_price_show"
        app:layout_constraintStart_toStartOf="@+id/view"
        app:layout_constraintTop_toBottomOf="@+id/tv_pay_monay_details_time" />

    <TextView
        android:id="@+id/tv_pay_monay_details_cost"
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="1dp"
        android:gravity="center_vertical"
        android:text="停车费用："
        android:textColor="@color/uuImportantText"
        app:layout_constraintEnd_toStartOf="@+id/tv_pay_monay_details_cost_show"
        app:layout_constraintStart_toStartOf="@+id/view"
        app:layout_constraintTop_toBottomOf="@+id/tv_pay_monay_details_price" />

    <TextView
        android:id="@+id/tv_pay_monay_details_time_length"
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="1dp"
        android:gravity="center_vertical"
        android:text="停车时长："
        android:textColor="@color/uuImportantText"
        app:layout_constraintEnd_toStartOf="@+id/tv_pay_monay_details_time_length_show"
        app:layout_constraintStart_toStartOf="@+id/view"
        app:layout_constraintTop_toBottomOf="@+id/tv_pay_monay_details_cost" />

    <TextView
        android:id="@+id/tv_pay_monay_details_order_id_show"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="16dp"
        android:gravity="center_vertical"
        app:layout_constraintBottom_toBottomOf="@+id/tv_pay_monay_details_order_id"
        app:layout_constraintEnd_toEndOf="@+id/view"
        app:layout_constraintStart_toEndOf="@+id/tv_pay_monay_details_order_id"
        app:layout_constraintTop_toTopOf="@+id/tv_pay_monay_details_order_id" />

    <TextView
        android:id="@+id/tv_pay_monay_details_lock_id_show"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="16dp"
        android:gravity="center_vertical"
        android:textColor="@color/uuImportantText"
        app:layout_constraintBottom_toBottomOf="@+id/tv_pay_monay_details_lock_id"
        app:layout_constraintEnd_toEndOf="@+id/view"
        app:layout_constraintStart_toEndOf="@+id/tv_pay_monay_details_lock_id"
        app:layout_constraintTop_toTopOf="@+id/tv_pay_monay_details_lock_id" />

    <TextView
        android:id="@+id/tv_pay_monay_details_location_show"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="16dp"
        android:gravity="center_vertical"
        android:text="TextView"
        android:textColor="@color/uuImportantText"
        app:layout_constraintBottom_toBottomOf="@+id/tv_pay_monay_details_location"
        app:layout_constraintEnd_toEndOf="@+id/view"
        app:layout_constraintStart_toEndOf="@+id/tv_pay_monay_details_location"
        app:layout_constraintTop_toTopOf="@+id/tv_pay_monay_details_location" />

    <TextView
        android:id="@+id/tv_pay_monay_details_time_show"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="16dp"
        android:gravity="center_vertical"
        android:text="TextView"
        android:textColor="@color/uuImportantText"
        app:layout_constraintBottom_toBottomOf="@+id/tv_pay_monay_details_time"
        app:layout_constraintEnd_toEndOf="@+id/view"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/tv_pay_monay_details_time"
        app:layout_constraintTop_toTopOf="@+id/tv_pay_monay_details_time" />

    <TextView
        android:id="@+id/tv_pay_monay_details_price_show"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="16dp"
        android:gravity="center_vertical"
        android:text="TextView"
        android:textColor="@color/uuImportantText"
        app:layout_constraintBottom_toBottomOf="@+id/tv_pay_monay_details_price"
        app:layout_constraintEnd_toEndOf="@+id/view"
        app:layout_constraintStart_toEndOf="@+id/tv_pay_monay_details_price"
        app:layout_constraintTop_toTopOf="@+id/tv_pay_monay_details_price" />

    <TextView
        android:id="@+id/tv_pay_monay_details_cost_show"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="16dp"
        android:gravity="center_vertical"
        android:text="TextView"
        android:textColor="@color/uuOrangeBtn"
        app:layout_constraintBottom_toBottomOf="@+id/tv_pay_monay_details_cost"
        app:layout_constraintEnd_toEndOf="@+id/view"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/tv_pay_monay_details_cost"
        app:layout_constraintTop_toTopOf="@+id/tv_pay_monay_details_cost" />

    <TextView
        android:id="@+id/tv_pay_monay_details_time_length_show"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="16dp"
        android:gravity="center_vertical"
        android:text="TextView"
        android:textColor="@color/uuOrangeBtn"
        app:layout_constraintBottom_toBottomOf="@+id/tv_pay_monay_details_time_length"
        app:layout_constraintEnd_toEndOf="@+id/view"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/tv_pay_monay_details_time_length"
        app:layout_constraintTop_toTopOf="@+id/tv_pay_monay_details_time_length" />

    <ImageView
        android:id="@+id/imageView21"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:layout_marginStart="24dp"
        app:layout_constraintBottom_toBottomOf="@+id/textView2"
        app:layout_constraintEnd_toStartOf="@+id/textView2"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="@+id/view"
        app:layout_constraintTop_toTopOf="@+id/textView2"
        app:srcCompat="@mipmap/dividing_line_back_horizontal" />

    <ImageView
        android:id="@+id/imageView22"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:layout_marginEnd="24dp"
        app:layout_constraintBottom_toBottomOf="@+id/textView2"
        app:layout_constraintEnd_toEndOf="@+id/view"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/textView2"
        app:layout_constraintTop_toTopOf="@+id/textView2"
        app:srcCompat="@mipmap/dividing_line_back_horizontal" />

    <TextView
        android:id="@+id/textView56"
        android:layout_width="0dp"
        android:layout_height="40dp"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:gravity="center"
        android:text="有疑问？"
        android:textColor="@color/uuImportantText"
        android:textSize="10sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/btn_pay_monay_details_sure"
        app:layout_constraintStart_toStartOf="@+id/btn_pay_monay_details_sure" />
</android.support.constraint.ConstraintLayout>